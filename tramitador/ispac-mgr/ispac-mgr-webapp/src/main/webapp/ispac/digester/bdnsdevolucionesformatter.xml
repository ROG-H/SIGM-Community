<?xml version="1.0" encoding="ISO-8859-1"?>
<beanformatter>

	<!-- Ponemos 2 veces la propiedad Nombre, 
		 una para mostrarla solo en html (media='html')
		 y la otra para la exportacion (media= 'xml excel csv pdf') -->
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='CIF_BENEFICIARIO' 
				 titleKey='BDNS_IGAE_DEVOLUCION:CIF_BENEFICIARIO' 
				 fieldType='LINK'
				 columnClass='width20percent'
				 headerClass='headerDisplay'
				 sortable='true'
				 media='html'/>
				 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='REFERENCIA_DEVOLUCION' 
				 titleKey='BDNS_IGAE_DEVOLUCION:REFERENCIA_DEVOLUCION' 
				 fieldType='LINK'
				 columnClass='width30percent'
				 headerClass='headerDisplay'
				 sortable='true'
				 media='html'/>

	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='IMPORTE' 
				 titleKey='BDNS_IGAE_DEVOLUCION:IMPORTE' 
				 fieldType='LIST' 
				 columnClass='width20percent'
				 headerClass='headerDisplay'
				 sortable='true'/>
				 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='FECHA_DEVOLUCION' 
				 titleKey='BDNS_IGAE_DEVOLUCION:FECHA_DEVOLUCION' 
				 fieldType='LIST' 
				 columnClass='width20percent'
				 headerClass='headerDisplay'
				 sortable='true'/>
				
</beanformatter>