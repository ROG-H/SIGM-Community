<?xml version="1.0" encoding="ISO-8859-1"?>
<beanformatter>

	<!-- Ponemos 2 veces la propiedad Nombre, 
		 una para mostrarla solo en html (media='html')
		 y la otra para la exportacion (media= 'xml excel csv pdf') -->
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='DNI' 
				 titleKey='DPCR_SERSO_TRAB_SOCIAL:DNI' 
				 fieldType='LINK'
				 columnClass='width10percent'
				 headerClass='headerDisplay'
				 sortable='true'
				 media='html'/>

	 <propertyfmt type="BeanPropertySimpleFmt" 
				 property='NOMBRE' 
				 titleKey='DPCR_SERSO_TRAB_SOCIAL:NOMBRE' 
				 fieldType='LINK'
				 columnClass='width40percent'
				 headerClass='headerDisplay'
				 sortable='true'
				 media='html'/>

	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='EMAIL' 
				 titleKey='DPCR_SERSO_TRAB_SOCIAL:EMAIL' 
				 fieldType='LIST' 
				 columnClass='width20percent'
				 headerClass='headerDisplay'
				 sortable='true'/>
				 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='MUNICIPIO' 
				 titleKey='DPCR_SERSO_TRAB_SOCIAL:MUNICIPIO' 
				 fieldType='LINK' 
				 columnClass='width30percent'
				 headerClass='headerDisplay'
				 sortable='true'/>
				 
</beanformatter>