				 headerClass='sortable'
				 sortable='true'/>
	
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='CAP_CONT_AUTORIZA' 
				 titleKey='CONTRATACION_PCAP_CONDADM:CAP_CONT_AUTORIZA' 
				 fieldType='LIST' 
				 columnClass='width15percent'
				 headerClass='sortable'
				 sortable='true'/>
				 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='CAP_CONT_DOC' 
