<?xml version="1.0" encoding="ISO-8859-1"?>
<beanformatter>

	<!-- Ponemos 2 veces la propiedad Nombre, 
		 una para mostrarla solo en html (media='html')
		 y la otra para la exportacion (media= 'xml excel csv pdf') -->
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='NUMEXP_EXPROPIADO' 
				 titleKey='EXPR_FINCA_EXPROPIADO_PAGO:NUMEXP_EXPROPIADO' 
				 fieldType='LINK'
				 columnClass='width40percent'
				 headerClass='sortable'
				 sortable='true'
				 media='html'/>
				 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='NUMEXP_EXPROPIADO' 
				 titleKey='EXPR_FINCA_EXPROPIADO_PAGO:NUMEXP_EXPROPIADO' 
				 fieldType='LIST'
				 columnClass='width40percent'
				 headerClass='headerDisplay'
				 media='csv excel xml pdf'/>
				 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='CANTIDADPAGO' 
				 titleKey='EXPR_FINCA_EXPROPIADO_PAGO:CANTIDADPAGO' 
				 fieldType='LINK'
				 columnClass='width40percent'
				 headerClass='sortable'
				 sortable='true'
				 media='html'/>
				 					 
</beanformatter>