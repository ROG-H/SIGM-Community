	<queryString><![CDATA[select 
	a.numexp as numexp_int_spac, d.numexp_expropiado as numexp_int_finca, a.nombre as nombre_int, c.numexp as numexp_finca, 
	c.num_poligono as numpolig_finca, c.num_parcela as numparcela_finca, c.municipio, c.sup_parcela, c.sup_expropiada,
	c.clase_finca as clasif_finca, c.importe, d.cantidadpago as total_justiprecio 
from 
	expr_fincas c 
join 
	expr_finca_expropiado_pago d 
		on c.numexp = d.numexp 
		and c.numexp in (
			select numexp_hijo 
			from 
				spac_exp_relacionados 
			where 
				numexp_padre = $P{NUM_EXP} 
				AND RELACION = 'Finca/Expropiacion')
left join 
	spac_dt_intervinientes a 
		on d.numexp_expropiado = a.numexp 
ORDER BY nombre_int]]></queryString>

