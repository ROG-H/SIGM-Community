	<queryString><![CDATA[select (regexp_split_to_array(b.codigo,'-'))[5]||'-'||(regexp_split_to_array(b.codigo,'-'))[6] as resolucion, b.factura, b.codigo 
,(regexp_split_to_array(b.codigo,'-'))[6]::integer as anioResol, substring((regexp_split_to_array(b.codigo,'-'))[5], 2, (regexp_split_to_array(b.codigo,'-'))[5].length)::integer as numeroResol
, b.proveedor as proveedor, b.cif as cif, b.fecha as fecha, b.importe as importe, municipio.sustituto as localidad, b.numexp as expediente
from dpcr_serso_cheques_recibidos as b 
join dpcr_serso_factura as factura on 
(case when factura.cif is null or factura.cif = '' then 1=2 else factura.cif = b.cif end)
left join dpcr_vldtbl_municipios municipio on factura.localidad = municipio.valor 
where (factura.cif = b.cif or factura.cif is null )
and factura.numexp=$P{NUM_EXP}
order by b.factura, anioResol, numeroResol]]></queryString>

