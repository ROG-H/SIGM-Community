<!DOCTYPE sqlMap
PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="Audit">

	<insert id="insertAudit"  parameterClass="es.msssi.sgm.registropresencial.beans.AuditBean">
		<selectKey keyProperty="idAudit"
             resultClass="java.lang.Integer">
		    SELECT SCR_SEQCNT.NEXTVAL AS idAudit FROM DUAL
		  </selectKey>
		INSERT INTO SCR_MODIFREG (ID, USR, MODIF_DATE, ID_FLD, NUM_REG, ID_ARCH, ID_OFIC, MODIF_TYPE)
        VALUES (#idAudit#, #name#, sysdate, #fldid#, #register#, #idBook#, #idOffice#, #modifType#)
	</insert>
	
	<insert id="insertAuditString"  parameterClass="es.msssi.sgm.registropresencial.beans.AuditBean">
		INSERT INTO SCR_VALSTR (ID,VALUE,OLDVALUE)
        VALUES (#idAudit#, #valueNew#, #valueOld#)
	</insert>
</sqlMap>