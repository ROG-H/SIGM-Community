<?xml version="1.0" encoding="ISO-8859-1"?>
<beanformatter pageSize='100'>

	<!-- Ponemos 2 veces la propiedad Asunto, 
		 una para mostrarla solo en html (media='html')
		 y la otra para la exportacion (media= 'xml excel csv pdf') -->
		 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='CRIT_ADJ' 
				 titleKey='CONTRATACION_CRIT_ADJ:CRIT_ADJ' 
				 fieldType='LINK'
				 columnClass='width20percent'
				 headerClass='sortable'
				 sortable='true'
				 media='html'/>
				 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='CRIT_ADJ' 
				 titleKey='CONTRATACION_CRIT_ADJ:CRIT_ADJ' 
				 fieldType='LIST'
				 columnClass='width20percent'
				 headerClass='headerDisplay'
				 media='csv excel xml pdf'/>
				 
				 


	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='PONDERACION' 
				 titleKey='CONTRATACION_CRIT_ADJ:PONDERACION' 
				 fieldType='LIST' 
				 columnClass='width20percent'
				 headerClass='sortable'
				 sortable='true'/>
	
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='DESCRIPCION' 
				 titleKey='CONTRATACION_CRIT_ADJ:DESCRIPCION' 
				 fieldType='LIST' 
				 columnClass='width60percent'
				 headerClass='sortable'
				 sortable='true'/>
				 
				 
				 
</beanformatter>