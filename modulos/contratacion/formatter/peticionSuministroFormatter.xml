<?xml version="1.0" encoding="ISO-8859-1"?>
<beanformatter pageSize='100'>

	<!-- Ponemos 2 veces la propiedad Asunto, 
		 una para mostrarla solo en html (media='html')
		 y la otra para la exportacion (media= 'xml excel csv pdf') -->
		 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='EMPRESA' 
				 titleKey='CONTRATACION_PETICION:EMPRESA' 
				 fieldType='LINK'
				 columnClass='width70percent'
				 headerClass='sortable'
				 sortable='true'
				 media='html'/>
				 
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='EMPRESA' 
				 titleKey='CONTRATACION_PETICION:EMPRESA' 
				 fieldType='LIST'
				 columnClass='width70percent'
				 headerClass='headerDisplay'
				 media='csv excel xml pdf'/>
				 
				 


	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='PRESUPUESTO' 
				 titleKey='CONTRATACION_PETICION:PRESUPUESTO' 
				 fieldType='LIST' 
				 columnClass='width10percent'
				 headerClass='sortable'
				 sortable='true'/>
	
	<propertyfmt type="BeanPropertySimpleFmt" 
				 property='FECHA_PEDIDO' 
				 titleKey='CONTRATACION_PETICION:FECHA_PEDIDO' 
				 fieldType='LIST' 
				 columnClass='width10percent'
				 headerClass='sortable'
				 sortable='true'/>
				 
				 
				 
</beanformatter>