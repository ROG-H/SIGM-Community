                <xs:annotation>
                    <xs:documentation xml:lang="en">Specifies reference to the attached resource </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="aclan:PlainTextType">
